{{#extends 'base'}}
    {{#block 'content'}}
    <h1>User Profile</h1>
    <table class="table">
        <thead>
            <tr>
                <th>UserName:</th>
                <th>Email:</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{user.username}}</td>
                <td>{{user.email}}</td>
            </tr>
        </tbody>
    </table>
    {{/block}}
{{/extends}}